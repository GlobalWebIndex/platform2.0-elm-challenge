{
  "name": "gwi-cats",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx} --max-warnings=0",
    "format": "prettier --write src/**/*.{js,jsx,json,css,scss,md}",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "postinstall": "husky install",
    "husky-add-pre-commit-hook": "husky add .husky/pre-commit \"npm run format && npm run lint\""
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.22.0",
    "primeicons": "^4.1.0",
    "primereact": "^6.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.1",
    "web-vitals": "^2.1.0",
    "workbox-background-sync": "^6.3.0",
    "workbox-broadcast-update": "^6.3.0",
    "workbox-cacheable-response": "^6.3.0",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^6.3.0",
    "workbox-google-analytics": "^6.3.0",
    "workbox-navigation-preload": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-range-requests": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "workbox-streams": "^6.3.0",
    "zustand": "^3.5.12"
  },
  "devDependencies": {
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  }
}
